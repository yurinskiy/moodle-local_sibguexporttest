        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="headerpage" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Верхний колонтитул"/>
